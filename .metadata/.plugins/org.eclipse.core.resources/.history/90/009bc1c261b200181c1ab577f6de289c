package org.green.crud1;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.jdbc.core.JdbcTemplate;
import org.springframework.stereotype.Repository;


@Repository
public class newTeacherDao {
	
	@Autowired
	private JdbcTemplate jTmp;
	
	
	public int insert(Teacher t){
		String sql = "INSERT INTO teacher (name, age, subject) VALUES ( ? , ? , ?)";	
		
		return jTmp.update(sql, t.getName(), t.getAge(), t.getSubject());
	}
		
}
